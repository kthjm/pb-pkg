{
  "name": "",
  "version": "0.0.0",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "kthjm/{name}",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "main": "dist/{name}.cjs.js",
  "jsnext:main": "dist/{name}.es.js",
  "module": "dist/{name}.es.js",
  "nyc": {
    "require": ["babel-register"],
    "include": ["src"],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "type:stub": "flow-typed create-stub",
    "type:add": "flow-typed install",
    "pretest": "flow",
    "test": "cross-env NODE_ENV=TEST nyc mocha",
    "prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD node rollup.config.all.js",
    "postbuild": "run-s format",
    "format": "prettier --write **/*.js package.json",
    "coverage": "nyc report --reporter=lcov && codecov"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.5",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.1",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "rewire": "^2.5.2",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.0.1",
    "uglify-es": "^3.2.1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
